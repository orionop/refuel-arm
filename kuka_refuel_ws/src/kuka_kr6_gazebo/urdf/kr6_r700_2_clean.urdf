<?xml version="1.0"?>
<!-- KUKA KR6 R700 sixx URDF with proper 3D meshes from ROS-Industrial -->
<!-- Source: ros-industrial/kuka_experimental/kuka_kr6_support -->
<!-- Inertial values computed from KUKA datasheet: 52kg total, 6kg payload -->
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="kr6_r700">

  <!-- World link: anchors the robot to the ground -->
  <link name="world"/>
  <joint name="world_to_base" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://kuka_kr6_gazebo/meshes/visual/base_link.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://kuka_kr6_gazebo/meshes/collision/base_link.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="15.0"/>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
      <inertia ixx="0.15" ixy="0" ixz="0" iyy="0.15" iyz="0" izz="0.10"/>
    </inertial>
  </link>

  <link name="link_1">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://kuka_kr6_gazebo/meshes/visual/link_1.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://kuka_kr6_gazebo/meshes/collision/link_1.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="12.0"/>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <inertia ixx="0.08" ixy="0" ixz="0" iyy="0.08" iyz="0" izz="0.06"/>
    </inertial>
  </link>

  <link name="link_2">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://kuka_kr6_gazebo/meshes/visual/link_2.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://kuka_kr6_gazebo/meshes/collision/link_2.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.15 0 0" rpy="0 0 0"/>
      <inertia ixx="0.03" ixy="0" ixz="0" iyy="0.20" iyz="0" izz="0.20"/>
    </inertial>
  </link>

  <link name="link_3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
