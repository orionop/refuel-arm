    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://kuka_kr6_gazebo/meshes/collision/link_6.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <!-- Joints â€” matching ROS-Industrial kinematic chain -->
  <!-- Added damping and friction for Gazebo stability -->
  <joint name="joint_1" type="revolute">
    <origin xyz="0 0 0.4" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <axis xyz="0 0 -1"/>
    <limit lower="-2.967059725" upper="2.967059725" effort="200" velocity="6.2832"/>
    <dynamics damping="10.0" friction="5.0"/>
  </joint>

  <joint name="joint_2" type="revolute">
    <origin xyz="0.025 0 0" rpy="0 0 0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.316125575" upper="0.785398163" effort="200" velocity="5.2360"/>
    <dynamics damping="10.0" friction="5.0"/>
  </joint>

  <joint name="joint_3" type="revolute">
    <origin xyz="0.315 0 0" rpy="0 0 0"/>
    <parent link="link_2"/>
    <child link="link_3"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.094395100" upper="2.722713630" effort="200" velocity="6.2832"/>
    <dynamics damping="10.0" friction="5.0"/>
  </joint>

  <joint name="joint_4" type="revolute">
    <origin xyz="0 0 0.035" rpy="0 0 0"/>
